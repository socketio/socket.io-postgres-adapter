{
  "name": "@socket.io/postgres-adapter",
  "version": "0.3.1",
  "description": "The Socket.IO Postgres adapter, allowing to broadcast events between several Socket.IO servers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:socketio/socket.io-postgres-adapter.git"
  },
  "files": [
    "dist/"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "npm run format:check && tsc && nyc mocha --require ts-node/register test/index.ts",
    "format:check": "prettier --parser typescript --check lib/**/*.ts test/**/*.ts",
    "format:fix": "prettier --parser typescript --write lib/**/*.ts test/**/*.ts",
    "prepack": "tsc"
  },
  "dependencies": {
    "@msgpack/msgpack": "~2.8.0",
    "@types/pg": "^8.6.6",
    "debug": "~4.3.4",
    "pg": "^8.9.0"
  },
  "peerDependencies": {
    "socket.io-adapter": "^2.4.0"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^10.0.1",
    "@types/node": "^14.14.7",
    "expect.js": "0.3.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.3",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "socket.io",
    "postgres",
    "postgresql",
    "adapter"
  ]
}
